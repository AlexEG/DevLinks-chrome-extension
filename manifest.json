{
  "name": "DevLinks",
  "description": "this is basically the same as bookmark folders!!",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./images/icon.png"
  },
  "icons": {
    "16": "./images/icon.png",
    "32": "./images/icon.png",
    "48": "./images/icon.png",
    "128": "./images/icon.png"
  },

  "content_scripts": [
    {
      "js": ["./scripts/content.js"],
      "matches": [
        "https://developer.chrome.com/docs/extensions/*",
        "https://developer.chrome.com/docs/webstore/*"
      ]
    }
  ]
}
